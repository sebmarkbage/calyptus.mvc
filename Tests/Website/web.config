<?xml version="1.0"?>
<configuration>
	<configSections>
		<section name="calyptus.mvc" type="Calyptus.MVC.Configuration.Config, Calyptus.MVC" />
	</configSections>
	<calyptus.mvc keywordExtension="" defaultViewEngine="Calyptus.MVC.ViewEngines.WebFormsViewEngine">
	</calyptus.mvc>
	<system.web>
		<compilation debug="true">
			<assemblies>
				<add assembly="System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
				<add assembly="System.Core, Version=3.5.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/></assemblies>
		</compilation>
		<authentication mode="Forms">
			<forms cookieless="AutoDetect"/>
		</authentication>
		<sessionState cookieless="AutoDetect">
		</sessionState>
		<customErrors mode="Off" />
		<pages>
			<controls>
			</controls>
		</pages>
		<httpModules>
			<add name="Calyptus.MVC" type="Calyptus.MVC.RoutingModule, Calyptus.MVC"/>
		</httpModules>
	</system.web>
	<system.webServer>
		<validation validateIntegratedModeConfiguration="false"/>
		<modules runAllManagedModulesForAllRequests="true">
			<add name="Calyptus.MVC" type="Calyptus.MVC.RoutingModule, Calyptus.MVC"/>
		</modules>
		<handlers>
			<add name="CalyptusMVCWildcardMap" preCondition="classicMode" verb="*" path="*.mvc" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework\v2.0.50727\aspnet_isapi.dll"/>
			<add name="CalyptusMVCHandler" preCondition="integratedMode" verb="*" path="Calyptus.MVC.axd" type="Calyptus.MVC.RoutingEngines.ControllerHandler, Calyptus.MVC"/>
		</handlers>
	</system.webServer>
	<system.codedom>
		<compilers>
			<compiler language="c#;cs;csharp" extension=".cs" compilerOptions="/warnaserror-" warningLevel="4" type="Microsoft.CSharp.CSharpCodeProvider, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
				<providerOption name="CompilerVersion" value="v3.5"/>
			</compiler>
			<compiler language="vb;vbs;visualbasic;vbscript" extension=".vb" compilerOptions="/optioninfer+" type="Microsoft.VisualBasic.VBCodeProvider, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
				<providerOption name="CompilerVersion" value="v3.5"/>
			</compiler>
		</compilers>
	</system.codedom>
</configuration>
